<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.renj.volleytest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".demo.StringDataActivity"/>
        <activity android:name=".demo.BeanDataActivity"/>
        <activity android:name=".demo.BytesDataActivity"/>
        <activity android:name=".demo.FormDataActivity"/>
        <activity android:name=".demo.ImageDataActivity"/>
    </application>

</manifest>